<section class="container">
<link href="https://fonts.cdnfonts.com/css/gold-lines-serif-trial" rel="stylesheet">
  <h3 class="gallery-name">{{gallery.name}}</h3>
  <!-- If the user is logged in, render the gallery content -->
  {{#if loggedIn}}
  <section class="flex-row justify-center">
    {{#each gallery.paintings as |painting|}}
    {{> painting-details}}
    {{/each}}
  </section>
</section>
<!-- Otherwise, provide a link to login -->
{{else}}
<a href="/login">You must login first to view Exercise Index</a>
{{/if}}
<style>
@import url('https://fonts.cdnfonts.com/css/gold-lines-serif-trial');
</style>
                
                     

